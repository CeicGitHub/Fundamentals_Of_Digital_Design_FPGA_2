module tb_alu4bit;
    reg [3:0] A, B, SEL;
    wire [7:0] RESULT;

    alu4bit uut (
        .A(A), .B(B), .SEL(SEL), .RESULT(RESULT)
    );

    initial begin
        $display("Time | A | B | SEL | RESULT");
        for (integer i = 0; i < 16; i = i + 1) begin
            A = 4'd3;
            B = 4'd2;
            SEL = i;
            #10 $display("%4t | %d | %d | %b | %d", $time, A, B, SEL, RESULT);
        end
        $stop;
    end
endmodule
